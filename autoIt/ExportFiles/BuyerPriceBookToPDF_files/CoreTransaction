//    debugger
//    GetTableDataRecord(0);
//    debugger
//}
function btnClose() {
    FILTER.Clear(83, 0, false);
    $("#btnAdvFilter").css({ "color": "#333" });
    $("#btnAdvFilterText").css({ "color": "#333" });
}
