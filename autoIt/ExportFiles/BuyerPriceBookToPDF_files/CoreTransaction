function Confirm(text) {
    debugger;
    text = text.toUpperCase();
    var rs = Button_enum.set(text);
    //alert(rs);
    //switch (rs.length > 0) {

    //}
};

// JavaScript Enum


var Button_enum = (function () {
    var getName = {
        0: 'YES',
        1: 'NO',
        2: 'OK',
        3: 'CANCEL',
        4: 'CONFIRM',
        5: 'YES,NO,CANCEL',
        6: 'YES,NO'
    };
    var SetNames = {
        'YES': 0,
        'NO': 1,
        'OK': 2,
        'CANCEL': 3,
        'CONFIRM': 4,
        'YESNOCANCEL': 5,
        'YESNo': 6
    };
    return {
        get: function (name) {
            return getName[name];
        },
        set: function (name) {
            return SetNames[name];
        },
        YES: 0,
        NO: 1,
        OK: 2,
        CANCEL: 3,
        CONFIRM: 4,
        YESNOCANCEL: 5,
        YESNO: 6
    }


})();


