    btnClearAuth_Click: function () {
        $("#btnSelectAllAuth").val("Select All");
        FGRIDCONTROL.clearData("GridPBAuthorize", true);
        $('#GridPBAuthorize> tbody> tr').removeClass('PriceBookSelectedRowColor');
        $('#myPagerAuthPopup button').remove();
