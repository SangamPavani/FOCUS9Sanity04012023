function OpenSplitPopUp_Success(flag,data) {
    try
    {
        if(flag && data!=null)
        {
            //debugger
            $('#AddEditsplitmodalpopup').html('');
            $('#AddEditsplitmodalpopup').html(data);
            $('#AddEditsplitmodalpopup').modal('show');
        }
    }catch(err)
    {
        COMMON.prototype.showMessage(err);
    }
}
