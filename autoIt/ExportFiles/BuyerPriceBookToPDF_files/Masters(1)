var EXTERNALMODULEFIELD_EVENT_ENUM = {
    ON_ENTER: 1,
    ON_LEAVE: 2
}
var Focus_Common_DataStructs_MasterExternalModuleEvents = (function () {
    var private = {
        "ALT_F1": 3,
        "ALT_F2": 4,
        "ALT_F3": 5,
        //  "ALT_F4": 6,
        "ALT_F5": 7,
        "ALT_F6": 8,
        "ALT_F7": 9,
        "ALT_F8": 10,
        "ALT_F9": 11,
        "ALT_F10": 12,
        "ALT_F11": 13,
        "ALT_F12": 14
    }
    return {
        get: function (name) { return private[name.toUpperCase()]; }
    };
})();
