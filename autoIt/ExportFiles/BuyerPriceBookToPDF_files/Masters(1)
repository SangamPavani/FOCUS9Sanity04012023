        //    var divId = $(ele).closest('div').attr('href');
        //    var numberPattern = /\d+/g;
        //    var multipleTreeCounter = $(divId).find('.clsdivSecondsub')[0].id;
        //    multipleTreeCounter = multipleTreeCounter.match(numberPattern);
