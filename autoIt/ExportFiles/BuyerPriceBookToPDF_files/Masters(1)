                    var myarrProdAttributeDtls = [];
                    for (var j = 0; j < $(attrTabId).find('#tblProductAttributesBody')[0].rows.length; j++) {
                        var arrObj = {
                            AttributeNo: "0",//$($(attrTabId).find('#tblProductAttributesBody')[0].rows[j].cells[1]).find('input').val(),
