var BUDGET = {
    optionCtrlLoaded: function (id, data, tag) {

        if (data != null) {
            if (data[0].sValue != 0) {
                $('#' + tag.CustomData).val(data[1].sValue);
                $('#' + tag.CustomData).attr("data-value", data[0].sValue)
                $('#' + tag.CustomData).attr("data-text", data[1].sValue)
            }
        }
    }
}
