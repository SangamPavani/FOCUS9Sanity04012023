            $("#AdvImportExport_ModalBody").html("");
            $("#AdvImportExport_ModalBody").html(data);
            $("#AdvImportExport_Modal").modal("show");

            var sCtrlId = document.getElementsByClassName("clsAdvImportFieldset");
