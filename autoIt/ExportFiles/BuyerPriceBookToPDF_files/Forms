            }
            else if (FormTypeId == 2) {
                debugger
                if ($('#chkDontMaintainStockbyBatch').is(':checked') && $('#chkBatch').is(':checked')) {
                    COMMON.prototype.showMessage("Cannot select reserve by batch with Donot Maintain Stock By Batch")
